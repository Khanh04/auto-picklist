{
  "name": "auto-picklist",
  "version": "1.0.0",
  "description": "Modern React web application for automated picklist generation with PostgreSQL database and intelligent supplier matching",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "web": "node server.js",
    "dev": "node src/app.js",
    "dev-web": "concurrently \"npm run build:watch\" \"npm run web\"",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "legacy": "node create_picklist.js",
    "setup-db": "node scripts/setup-database.js",
    "import-data": "node scripts/import-excel-data.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "picklist",
    "supplier",
    "automation",
    "postgresql",
    "procurement",
    "pdf",
    "csv",
    "web-app"
  ],
  "author": "Auto Picklist Generator",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
