{
  "name": "auto-picklist",
  "version": "1.0.0",
  "description": "Modern React web application for automated picklist generation with PostgreSQL database and intelligent supplier matching",
  "main": "src/app.js",
  "scripts": {
    "start": "node server.js",
    "start:legacy": "node server-legacy.js",
    "dev": "node src/app.js",
    "dev-web": "concurrently \"npm run build:watch\" \"npm run web\"",
    "web": "node server.js",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "legacy": "node create_picklist.js",
    "setup-db": "node scripts/setup-database.js",
    "import-data": "node scripts/import-excel-data.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t auto-picklist .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "react",
    "picklist",
    "supplier",
    "automation",
    "postgresql",
    "procurement",
    "pdf",
    "csv",
    "web-app"
  ],
  "author": "Auto Picklist Generator",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "express": "^4.18.2",
    "fuse.js": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "react-select": "^5.10.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "jest": "^30.0.5",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "vite": "^5.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}
